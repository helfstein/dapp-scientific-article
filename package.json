{
  "name": "dapp-scientific-article",
  "version": "1.0.0",
  "description": "Quick start guide to initialize a new private network with Geth",
  "main": "index.js",
  "scripts": {
    "setup": "cd ./ethereumbridgenode && npm i && cd ../src && npm i && cd dapp && npm i -f",
    "start-ganache": "ganache-cli -p 7545 --seed abcd --defaultBalanceEther 1000",
